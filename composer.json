{
    "name": "ibt/practice",
    "type": "project",
    "require": {
        "brutaldesign/swipebox": "^1.0",
        "cweagans/composer-patches": "^1.6.0",
        "drupal/admin_toolbar": "^1.23",
        "drupal/block_class": "^1.0@alpha",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.4",
        "drupal/devel": "^1.2",
        "drupal/drush_delete": "^2.2",
        "drupal/paragraphs": "^1.2",
        "drupal/physical": "^1.0@RC",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/superfish": "^1.2",
        "drush/drush": "~8.0|^9.0.0-beta8",
        "hussainweb/drupal-composer-helper": "^1.0"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8"
    },
    "authors": [
        {
            "name": "Isaac Bigsby Trogdon",
            "email": "mr@dyss.net"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "brutaldesign/swipebox",
                "version": "1.0",
                "dist": {
                    "url": "https://github.com/brutaldesign/swipebox/archive/master.zip",
                    "type": "zip"
                },
                "type": "drupal-library"
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dotist/d8base_ui",
                "version": "1.0",
                "dist": {
                    "url": "https://github.com/dotist/d8base_ui/archive/master.zip",
                    "type": "zip"
                },
                "type": "drupal-custom-theme"
            }
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "drupal-composer-helper": {
            "web-prefix": "www"
        },
        "enable-patching": true,
        "installer-paths": {
            "www/core": [
                "type:drupal-core"
            ]
        }
    },
    "prefer-stable": true,
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "installer-paths": {
            "core": ["type:drupal-core"],
            "libraries/{$name}": ["type:drupal-library"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/{$name}": ["type:drupal-theme"],
            "drush/{$name}": ["type:drupal-drush"],
            "modules/custom/{$name}": ["type:drupal-custom-module"],
            "themes/{$name}": ["type:drupal-custom-theme"]
        }
    }
}
